{
  "name": "sitcon-2020",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run generate:session && vue-cli-service serve",
    "build": "npm run generate:session && npm run generate:mail && vue-cli-service build && mv ./dist/2020/* ./dist && rm -rf ./dist/2020 && rm ./dist/index.html && cp -R ./dist/zh-TW/* ./dist/",
    "lint": "vue-cli-service lint",
    "generate:mail": "heml build template/mail/SITCON-2020.heml --output public/mail/SITCON-2020.html && cp -R template/mail/assets public/mail/",
    "generate:session": "node src/util/session/index.js > public/json/session.json",
    "serve:mail": "heml develop template/mail/SITCON-2020.heml"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "spreadsheet-to-json": "^1.3.1",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@ccip-app/session-table": "^1.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "@types/lodash": "^4.14.149",
    "@types/ol": "^6.1.0",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "axios": "^0.19.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "heml": "^1.1.3",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "markdown": "^0.5.0",
    "ol": "^6.1.1",
    "prerender-spa-plugin": "^3.4.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "typescript": "^3.4.3",
    "vue-cli-plugin-prerender-spa": "^1.1.6",
    "vue-cookies": "^1.7.0",
    "vue-template-compiler": "^2.6.10",
    "vuex-class": "^0.3.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
